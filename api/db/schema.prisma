datasource DS {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = "native"
}

// Define your own models here and run `yarn rw db save` to create
// migrations for them.

model Todo {
  id     Int    @id @default(autoincrement())
  body   String
  status String @default("off")
}
